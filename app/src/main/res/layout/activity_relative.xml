<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <EditText
        android:id="@+id/firstET"
        android:layout_width="128dp"
        android:layout_height="100dp"
        android:background="#1be614"/>
    <EditText
        android:id="@+id/secondET"
        android:layout_width="128dp"
        android:layout_height="50dp"
        android:background="#da3117"
        android:layout_toRightOf="@id/firstET"/>

    <EditText
        android:id="@+id/thirdET"
        android:layout_width="128dp"
        android:layout_height="50dp"
        android:background="#201d1d"
        android:layout_toRightOf="@id/firstET"
        android:layout_below="@+id/secondET"/>
    <EditText
        android:id="@+id/fourthET"
        android:layout_width="128dp"
        android:layout_height="100dp"
        android:background="#13ac2f"
        android:layout_toRightOf="@id/thirdET" />
    <EditText
        android:id="@+id/fifthET"
        android:layout_width="128dp"
        android:layout_height="100dp"
        android:background="#33179b"
        android:layout_below="@id/firstET"/>
    <EditText
        android:id="@+id/sixthET"
        android:layout_width="128dp"
        android:layout_height="100dp"
        android:background="#ec10ec"
        android:layout_toRightOf="@id/fifthET"
        android:layout_below="@id/thirdET"/>
    <EditText
        android:id="@+id/sevenththET"
        android:layout_width="128dp"
        android:layout_height="100dp"
        android:background="#18167b"
        android:layout_toRightOf="@id/sixthET"
        android:layout_below="@id/thirdET"/>

    <EditText
        android:id="@+id/eighthET"
        android:layout_width="96dp"
        android:layout_height="100dp"
        android:background="#3edc26"
        android:layout_below="@id/fifthET"/>

    <EditText
        android:id="@+id/ninethET"
        android:layout_width="96dp"
        android:layout_height="33.33dp"
        android:background="#e40d2d"
        android:layout_toRightOf="@+id/eighthET"
        android:layout_below="@id/fifthET"/>
    <EditText
        android:id="@+id/tenthET"
        android:layout_width="96dp"
        android:layout_height="33.33dp"
        android:background="#181617"
        android:layout_toRightOf="@id/eighthET"
        android:layout_below="@id/ninethET"/>
    <EditText
        android:id="@+id/eleventhET"
        android:layout_width="96dp"
        android:layout_height="33.33dp"
        android:background="#e90e0e"
        android:layout_toRightOf="@id/eighthET"
        android:layout_below="@id/tenthET"/>

    <EditText
        android:id="@+id/twelvethET"
        android:layout_width="96dp"
        android:layout_height="100dp"
        android:background="#b3aba7"
        android:layout_below="@id/fifthET"
        android:layout_toRightOf="@id/ninethET"/>
    <EditText
        android:id="@+id/thirteenthET"
        android:layout_width="96dp"
        android:layout_height="33.33dp"
        android:background="#ded00f"
        android:layout_below="@id/fifthET"
        android:layout_toRightOf="@id/twelvethET"/>
    <EditText
        android:id="@+id/forteenthET"
        android:layout_width="96dp"
        android:layout_height="33.33dp"
        android:background="#e94a31"
        android:layout_below="@id/thirteenthET"
        android:layout_toRightOf="@id/twelvethET"/>
    <EditText
        android:id="@+id/fifteenthET"
        android:layout_width="96dp"
        android:layout_height="33.33dp"
        android:background="#c50fde"
        android:layout_below="@id/forteenthET"
        android:layout_toRightOf="@id/twelvethET"/>
    <EditText
        android:id="@+id/sixteenthET"
        android:layout_width="96dp"
        android:layout_height="100dp"
        android:background="#182864"
        android:layout_below="@id/fifteenthET"/>
    <EditText
        android:id="@+id/seventeenthET"
        android:layout_width="96dp"
        android:layout_height="100dp"
        android:background="#0dd2ec"
        android:layout_below="@id/eleventhET"
        android:layout_toRightOf="@id/sixteenthET"/>
    <EditText
        android:id="@+id/eighteenthET"
        android:layout_width="96dp"
        android:layout_height="100dp"
        android:background="#d679a7"
        android:layout_below="@id/twelvethET"
        android:layout_toRightOf="@id/seventeenthET"/>
    <EditText
        android:id="@+id/nineteenthET"
        android:layout_width="96dp"
        android:layout_height="100dp"
        android:background="#3edc26"
        android:layout_below="@id/fifteenthET"
        android:layout_toRightOf="@id/eighteenthET"/>
    <EditText
        android:id="@+id/twentythET"
        android:layout_width="192dp"
        android:layout_height="55dp"
        android:background="#d70ce2"
        android:layout_below="@id/sixteenthET"/>
    <EditText
        android:id="@+id/twentyonethET"
        android:layout_width="192dp"
        android:layout_height="55dp"
        android:background="#ec0a20"
        android:layout_below="@id/twentythET"/>
    <EditText
        android:id="@+id/twentytwothET"
        android:layout_width="96dp"
        android:layout_height="110dp"
        android:background="#efe058"
        android:layout_below="@+id/eighteenthET"
        android:layout_toRightOf="@id/twentyonethET"
        android:layout_toLeftOf="@id/nineteenthET"
        android:layout_toStartOf="@id/nineteenthET" />
    <EditText
        android:id="@+id/twentythreethET"
        android:layout_width="96dp"
        android:layout_height="55dp"
        android:background="#de660b"
        android:layout_below="@id/nineteenthET"
        android:layout_toRightOf="@id/twentytwothET"/>
    <EditText
        android:id="@+id/twentyfourthET"
        android:layout_width="96dp"
        android:layout_height="55dp"
        android:background="#0ee9e9"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_toRightOf="@id/twentytwothET"
        android:layout_toEndOf="@id/twentytwothET" />

</RelativeLayout>
